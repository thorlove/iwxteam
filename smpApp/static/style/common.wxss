stv-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.withAnimate {
  transition: all 100ms ease;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-perspective: 1000;
  perspective: 1000;
}

.stv-container .tab-bar {
  position: relative;
  display: flex;
  font-size: 24rpx;
  color: #c9c9c9;
}

.stv-container .stv-opt {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.stv-opt-date {
  width: 80%;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}

.stv-container .tab-bar .tab-active {
  color: #fff;
  z-index: 1;
  font-weight: bold;
}

.stv-container .tab-bar .tab {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.stv-container .tab-bar .under-line {
  position: absolute;
  bottom: 0;
  height: 4rpx;
  border-radius: 8rpx;
  background-color: #f5f5f9;
}

.stv-container .scroll-view {
  position: relative;
  width: 100%;
  height: 150px;
  background: #554d94;
}

.stv-container .scroll-view .scroll-view-wrapper {
  position: absolute;
  top: 0;
  bottom: 0;
  display: flex;
}

.stv-container .scroll-view .scroll-view-wrapper .one-scene {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.one-scene .container {
  background-color: #fff;
  border-radius: 10rpx;
  width: 96%;
  height: 90%;
  flex-direction: row;
  justify-content: space-around;
  align-items:center;
}

.data-content {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}